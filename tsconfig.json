{
  "compilerOptions": {
    "moduleResolution": "Node",
    "module": "ESNext",
    "target": "ESNext",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext.Array",
      "ESNext"
    ],
    "downlevelIteration": true,
    "strictNullChecks": true,
    "strictBindCallApply": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "jsx": "preserve",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": [
    "pages"
  ],
  "exclude": [
    "node_modules"
  ]
}
